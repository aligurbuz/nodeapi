    <link rel="icon" type="image/png" sizes="16x16" href="../plugins/images/favicon.png">
    <title>Elite Admin - is a responsive admin template</title>
    <!-- Bootstrap Core CSS -->
    <link href="{{ base.static }}/elite/temp1/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">
    <!-- animation CSS -->
    <link href="{{ base.static }}/elite/temp1/css/animate.css" rel="stylesheet">
    <!-- Custom CSS -->
